<div class="container">
  <div class="row">
    <div class="panel panel-primary">
      <div class="panel-heading">Nouvelle liste</div>
      <div class="panel-body">
        <div class="col-md-6">
        <%= simple_form_for @list do |f| %>
          <%= f.input :name%>
          <%= f.input :photo, as: :file %>
          <%= f.submit %>
        <% end %>
      </div>
      </div>
    </div>
  </div>
<%= link_to "All Lists", lists_path, class: "btn btn-ghost" %>

</div>
